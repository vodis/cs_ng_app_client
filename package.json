{
  "name": "cs-ng-app-client",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-dev": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration development  --aot && gulp compress",
    "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration production  --aot && gulp compress",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint --ext .ts --ext .html ./ --fix",
    "lint-fix": "ng lint --fix",
    "format": "prettier --write ./",
    "stylelint-check": "stylelint src/**/*scss",
    "stylelint-fix": "stylelint src/**/*scss --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.3",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.3",
    "@angular/compiler": "^17.1.3",
    "@angular/core": "^17.1.3",
    "@angular/forms": "^17.1.3",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^17.1.3",
    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/router": "^17.1.3",
    "@vodis/ui-kit": "^1.0.15",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "^17.2.1",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.3",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.2",
    "stylelint": "^16.1.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{scss}": [
      "npm run stylelint-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
